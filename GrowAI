echo "# GrowAI" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/ShujaAziz/GrowAI.git
git push -u origin main
npm install gh-pages --save-dev
"homepage": "https://ShujaAziz.github.io/growai",
"scripts": {
  "deploy": "gh-pages -d dist",
  "predeploy": "npm run build"
}
npm run deploy

